.up_img_wrap {
    width: 90rpx;
    height: 90rpx;
    position: relative;
    border: 1px solid #dedede;
    border-radius: 15rpx;
}
.up_img_wrap .img {
    width: 100%;
    height: 100%;
    border-radius: 15rpx;
}
.up_img_wrap .clear {
    position: absolute;
    top: -22rpx;
    right: -22rpx;
}